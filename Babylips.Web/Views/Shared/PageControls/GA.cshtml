@if (ConfigManager.GetInstance().GAEnabled) {
<script>
    var trackingID = 'UA-27815696-31';
    var baseDomain = 'maybelline.com.tr';
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', trackingID, baseDomain);
    ga('send', 'pageview');
</script>
}
<script>
    $(function () {
        $('a').on('mousedown', function () {
            if ($(this).attr("data-ga") != undefined)
                GATracker($(this).attr("data-ga"));
        });
    });
    function GATracker(eventLabel) {
        try {
            if (ga != null || ga != undefined) {
                ga('send', 'event', 'button', 'click', eventLabel)
                //console.log("GA: " + eventLabel);
            }
        }
        catch (err) { }
    }
</script>
